  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "convene": "0.1.3",
    "express": "^4.17.1"
  },
  "dependencies": {
    "@srcer/events": "^2.1.3"
  }
